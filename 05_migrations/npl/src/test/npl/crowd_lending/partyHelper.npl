package crowd_lending

function getJean() returns Party -> {
    return partyOf(
        mapOf(
            Pair("iss", setOf(ISS)),
            Pair("email", setOf("jean@noumenadigital.com"))
        ),
        mapOf(
            Pair("department", setOf("Something"))
        )
    );
};

